{
  "name": "covid-19-insights-graphql-api",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn w webpack.prod.js",
    "postinstall": "yarn patch-package",
    "lint": "eslint --fix --ext .js,.ts,.tsx",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx .",
    "prepare": "husky install",
    "prettier": "prettier",
    "prestart": "yarn prisma generate",
    "start": "yarn w webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "update-schema": "node webpackx ./scripts/updateSchema.ts",
    "w": "webpack --progress --config",
    "docker:restart": "rm -rf temp && docker-compose down && docker rmi -f $(docker images -aq); docker-compose up"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@prisma/client": "^4.14.1",
    "date-fns": "^2.30.0",
    "dotenv-safe": "^8.2.0",
    "graphql": "16.6.0",
    "graphql-add-middleware": "^0.3.7",
    "graphql-playground-middleware": "^1.1.2",
    "graphql-scalars": "^1.21.3",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.0",
    "koa-graphql": "^0.12.0",
    "koa-logger": "^3.2.1",
    "prisma": "^4.14.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/jest": "^29.5.1",
    "@types/koa": "^2.13.6",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-graphql": "^0.8.7",
    "@types/koa-logger": "^3.1.2",
    "@types/koa__cors": "^4.0.0",
    "@types/koa__router": "^12.0.0",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "esbuild": "^0.17.19",
    "esbuild-jest": "^0.5.0",
    "esbuild-loader": "^3.0.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "patch-package": "^7.0.0",
    "prettier": "^2.8.8",
    "sort-package-json": "^2.4.1",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.2"
  }
